<div id="crypto-show-wrapper">
    <h1><%= @crypto.name %></h1>
    <br>
    <h5 id="cmc-rank">Yo</h5>
    <h5 id="crypto-price"></h5>
    <h5 id="market-cap"></h5>
    <h5 id="circ-supply"></h5>
    <h5 id="max-supply"></h5>
    <br>
    <%= link_to "Back", cryptos_path, id: "back-btn" %>
    <%= link_to "Edit", edit_crypto_path(@crypto) %>
    <%= link_to "Delete" , crypto_path(@crypto), method: :delete,
                data: { confirm: "Are you sure?" } %>
</div>